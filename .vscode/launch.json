    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "dlvFlags": [
                "--check-go-version=false"
            ],
            "env": {},
            "showLog": false
        },
        {
        "name": "Run Current File",
        "type": "go",
        "request": "launch",
        //   "goproxy": "https://goproxy.cn,direct",
        "mode": "auto", // 自动判断是运行还是调试
        "program": "${file}" // 指向当前打开的文件
        }
    ]
}
